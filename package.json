{
  "name": "@buildit/storybook-addon-blabbr",
  "version": "1.0.0",
  "description": "Component review and commenting for React Storybook",
  "main": "dist/register.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook",
    "lint": "./node_modules/prettier/bin/prettier.js --single-quote --list-different '{src,__{tests,mocks}__}/**/*.js'",
    "lint-fix": "./node_modules/prettier/bin/prettier.js --single-quote --write '{src,__{tests,mocks}__}/**/*.js'",
    "build-storybook": "build-storybook",
    "transpile": "babel ./src --out-dir dist --copy-files",
    "test": "jest ./src",
    "build": "npm run test && npm run transpile",
    "prepush": "npm run lint && npm test",
    "prepublish": "rm -fr dist && npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildit/storybook-addon-blabbr.git"
  },
  "keywords": [
    "atomic-design",
    "blabbr",
    "react",
    "storybook",
    "storybook-addon",
    "storybook-addon-blabbr",
    "collaboration",
    "review"
  ],
  "author": "Buildit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildit/storybook-addon-blabbr/issues"
  },
  "homepage": "https://github.com/buildit/storybook-addon-blabbr#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.2",
    "@storybook/addon-knobs": "^3.1.2",
    "@storybook/addons": "^3.1.2",
    "@storybook/react": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.7.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "prettier": "^1.4.4",
    "prop-types": "^15",
    "react": "^15",
    "react-dom": "^15",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "events": "^1.1.1",
    "marked": "^0.3.6",
    "pouchdb-browser": "^6.1.1",
    "pouchdb-find": "^6.2.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-alert": "^2.0.1",
    "react-tooltip": "^3.2.7"
  },
  "peerDependencies": {
    "@storybook/react": "^3.1.3",
    "@storybook/addons": "^3.1.2",
    "css-loader": "^0.28.4",
    "prop-types": "^15",
    "react": "^15",
    "react-dom": "^15",
    "style-loader": "^0.17.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "getConfig": "<rootDir>/__mocks__/getConfig.js"
    }
  }
}
