{
  "name": "@buildit/storybook-addon-blabbr",
  "version": "0.3.6",
  "description": "Component review and commenting for React Storybook",
  "main": "dist/index.js",
  "bin": {
    "blabbr-config": "./scripts/configure.js"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook",
    "lint": "eslint src --ext .js",
    "transpile": "babel ./src --out-dir dist --copy-files",
    "test": "jest",
    "build": "npm run lint && npm run test && npm run transpile",
    "prepublish": "rm -fr dist && npm run transpile",
    "configure": "node ./scripts/configure.js",
    "postinstall": "node ./scripts/check-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildit/storybook-addon-blabbr.git"
  },
  "keywords": [
    "atomic-design",
    "blabbr",
    "react",
    "storybook",
    "storybook-addon",
    "storybook-addon-blabbr",
    "collaboration",
    "review"
  ],
  "author": "Buildit",
  "contributors": [
    {
      "name": "Panos Voudouris",
      "url": "https://github.com/panosvoudouris"
    },
    {
      "name": "Jonathan Ellis-Craig",
      "url": "https://github.com/jon-ec"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildit/storybook-addon-blabbr/issues"
  },
  "homepage": "https://github.com/buildit/storybook-addon-blabbr#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addons": "^1.6.1",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "jest": "^20.0.3",
    "json-loader": "^0.5.4",
    "prop-types": "^15",
    "react": "^15",
    "react-dom": "^15",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "events": "^1.1.1",
    "fs-extra": "^3.0.1",
    "inquirer": "^3.0.6",
    "marked": "^0.3.6",
    "pouchdb-browser": "^6.1.1",
    "pouchdb-find": "^6.2.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-alert": "^2.0.1",
    "react-tooltip": "^3.2.7"
  },
  "peerDependencies": {
    "@kadira/storybook": "^2.35.2",
    "@kadira/storybook-addons": "^1.6.1",
    "css-loader": "^0.28.1",
    "file-loader": "^0.11.1",
    "prop-types": "^15",
    "react": "^15",
    "react-dom": "^15",
    "style-loader": "^0.17.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "blabbr-config": "<rootDir>/.storybook/blabbr-config.js"
    }
  }
}
